<div class="row">
    <div class="col-md-12">
        <h2>Images</h2>
        <hr />
    </div>
    <div class="col-md-4">
        {{#each data}}
            <img src="{{this}}" class="img-responsive img-thumbnail" alt="{{this}}"/>

        {{else}}
            <div class="col-md-12 text-center">
                <strong>No Images Available</strong>
            </div>
        {{/each}}
    </div>
</div>